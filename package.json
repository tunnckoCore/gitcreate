{
  "name": "gitcreate",
  "description": "Create a Github repository with only `username/repo`. Support CLI and Promises.",
  "version": "0.0.4",
  "scripts": {
    "lint": "npm install && node_modules/.bin/jshint . && node_modules/.bin/jscs . --reporter inline",
    "test": "npm run lint && node_modules/.bin/mocha --require should",
    "test-cov": "npm run lint && node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- --require should",
    "test-travis": "npm run lint && node_modules/.bin/istanbul cover node_modules/.bin/_mocha --report lcovonly -- --require should",
    "coveralls": "npm run test-travis && cat coverage/lcov.info | node_modules/.bin/coveralls"
  },
  "author": {
    "name": "Charlike Make Reagent",
    "email": "mameto_100@mail.bg",
    "url": "https://github.com/tunnckoCore"
  },
  "keywords": [
    "promises",
    "promise",
    "git",
    "cli",
    "support",
    "helper",
    "github",
    "utils",
    "utility",
    "util",
    "create",
    "repo",
    "repository"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/tunnckoCore/gitcreate.git"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/tunnckoCore/gitcreate/blob/master/license.md"
  },
  "dependencies": {
    "meow": "*",
    "got": "*",
    "native-or-another": "*"
  },
  "devDependencies": {
    "micropromise": "*",

    "coveralls": "^2.11.2",
    "istanbul": "^0.3.2",
    "jscs": ">=1.8.0",
    "jscs-jsdoc": ">=0.2.0",
    "jshint": "^2.5.10",
    "mocha": "^2.0.1",
    "mocha-lcov-reporter": "0.0.1"
  },
  "engines": {
    "node": ">= 0.10.33",
    "npm": ">= 1.4.28"
  },
  "bin": {
    "2gitcreate2": "bin/cli.js"
  },
  "preferGlobal": "true"
}
