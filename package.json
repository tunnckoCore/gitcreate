{
  "name": "gitcreate",
  "version": "0.0.4",
  "description": "Create a Github repository with only `username/repo`. Support CLI and Promises.",
  "main": "index.js",
  "scripts": {
    "lint": "npm install && jshint bin/*.js lib/*.js test/*.js index.js",
    "test": "npm run lint && mocha",
    "test-cov": "npm test && istanbul cover _mocha",
    "test-travis": "npm test && istanbul cover _mocha --report lcovonly"
  },
  "author": {
    "name": "Charlike Make Reagent",
    "email": "mameto_100@mail.bg",
    "url": "https://github.com/tunnckoCore",
    "twitter": "https://twitter.com/tunnckoCore"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tunnckoCore/gitcreate.git"
  },
  "keywords": [
    "promises",
    "promise",
    "git",
    "cli",
    "support",
    "helper",
    "github",
    "utils",
    "utility",
    "util",
    "create",
    "repo",
    "repository"
  ],
  "dependencies": {
    "meow": "*",
    "superagent": "*",
    "native-or-another": "*"
  },
  "devDependencies": {
    "coveralls": "*",
    "istanbul": "*",
    "jshint": "*",
    "mocha": ">=2",
    "mocha-lcov-reporter": "*",
    "micropromise": "*"
  },
  "licenses": [{
    "type": "MIT",
    "url": "https://github.com/tunnckoCore/gitcreate/blob/master/license.md"
  }],
  "engines": {
    "node": ">= 0.10.0",
    "npm": ">= 1.4.28"
  },
  "bin": {
    "gitcreate": "bin/cli.js"
  },
  "preferGlobal": "true"
}
